 "html": "{% set result = frappe.call('smartclaims.api.create.get_general_ledger_prepared_data') %}\n{% set filters = result.applied_filters %}\n{% set party = filters.party[0] if filters.party else filters.party_name %}\n{%if party%}\n{% set supplier_doc = frappe.get_doc(\"Supplier\", party) %}\n{% set address = supplier_doc.custom_postal_address or \"\" %}\n{%endif%}\n{% set balances = result.balance_details %}\n{% set data = result.data %}\n{% set from_date = frappe.utils.formatdate(filters.from_date, \"dd MMM yyyy\") %}\n{% set to_date = frappe.utils.formatdate(filters.to_date, \"dd MMM yyyy\") %}\n\n\n<table>\n    <tbody>\n    <tr>\n    <td colspan=\"4\"><img src=\"/files/company_logo.png\" alt=\"Company Logo\" width=\"220\" height=\"85\"></td>\n    <td colspan=\"8\" style=\"font-weight:bold;line-height: 1.3\">\n        <span style=\"font-size:14px;\">NATIONWIDE MEDICAL INSURANCE CO. LTD.<br></span>\n        <span style=\"font-size:12px; line-height: 1.3;\">PMB 108 K.I.A ACCRA Digital Address: GA-053-5737<br></span>\n        <span style=\"font-size:10px; line-height: 1.3;\">Tel: (0302) 226 892 Toll Free: (0800) 222 222<br>\n        Email: finance@nationwidemh.com<br>\n        Website: www.nationwidemh.com</span>\n    </td>\n    </tr>\n    <tr>\n        <td></td>\n    </tr>\n</tbody></table>\n\n<table style=\"margin-top:-10px; table-layout:fixed; width:100%;\">\n<tr>\n    <td colspan=\"14\" rowspan=\"2\" style=\"vertical-align:middle !important;text-align:center;font-weight:bold;\">SERVICE PROVIDER LEDGER</td>\n    <td style=\"font-size:12px; text-align:right;\" colspan=\"4\"><b>From</b> : {{from_date or \"\"}}</td>\n</tr>\n<tr>\n    <td  style=\"font-size:12px; text-align:right;\" colspan=\"4\"><b>To</b> : {{to_date or \"\"}}</td>\n</tr>\n</table>\n\n\n<table style=\"margin-top:-5px;table-layout:fixed; width:100%;border-top:1px solid black;font-size:11px;\">\n    <tr>\n        <td rowspan=\"2\" colspan=\"2\" style=\"text-align:left;\">\n         <b>ID:</b>&nbsp;\n        </td>\n         <td colspan=\"10\" style=\"text-align:left;\">\n         <b>Name:</b>&nbsp;{{party or \"\"}}\n        </td>\n    </tr>\n    <tr>\n       <td colspan=\"10\" style=\"text-align:left;\">\n         <b>Address:</b>&nbsp;{{address or \"\"}}\n        </td>\n    </tr>\n</table>\n<table style=\"table-layout:fixed; width:100%;margin-top:10px;border-top:1px solid black;font-size:11px;\">\n    <tr>\n        <td colspan=\"2\" style=\"text-align:left;border-bottom:1px solid black;\">\n         <b>Date</b>\n        </td>\n         <td colspan=\"5\" style=\"text-align:left;border-bottom:1px solid black;\">\n         <b>Narration</b>\n        </td>\n        <td colspan=\"2\" style=\"text-align:right;border-bottom:1px solid black;\">\n         <b>Cheque#</b>\n        </td>\n        <td colspan=\"2\" style=\"text-align:right;border-bottom:1px solid black;\">\n         <b>Debit</b>\n        </td>\n        <td colspan=\"2\" style=\"text-align:right;border-bottom:1px solid black;\">\n         <b>Credit</b>\n        </td>\n        <td colspan=\"2\" style=\"text-align:right;border-bottom:1px solid black;\">\n         <b>Balance<br>GHS</b>\n        </td>\n    </tr>\n\n    <tr>\n        <td colspan=\"2\" style=\"text-align:left;\"></td>\n         <td colspan=\"5\" style=\"text-align:left;\">Balance Brought Forward</td>\n        <td colspan=\"2\" style=\"text-align:center;\"></td>\n        <td colspan=\"2\" style=\"text-align:center;\"></td>\n        <td colspan=\"2\" style=\"text-align:center;\"></td>\n        <td colspan=\"2\" style=\"text-align:right;\">({{ balances.opening.balance or \"0.0\" }})</td>\n    </tr>\n   \n    {% for i in data %}\n    <tr style=\" page-break-inside:avoid !important; background-color:{% if loop.index is odd %}#dedede{% endif %};\">\n        <td colspan=\"2\" style=\"text-align:left;\">{{ frappe.utils.formatdate(i.posting_date, \"dd MMM yyyy\") }}</td>\n        <td colspan=\"5\" style=\"text-align:left;\">{{i.remarks or \"Please Check Show Remark from the Report\"}}</td>\n        <td colspan=\"2\" style=\"text-align:right;\">{{i.bill_no or \"\"}}</td>\n        <td colspan=\"2\" style=\"text-align:right;\">{{ \"{:,.2f}\".format(i.debit or 0) }}</td>\n        <td colspan=\"2\" style=\"text-align:right;\">{{ \"{:,.2f}\".format(i.credit or 0) }}</td>\n        <td colspan=\"2\" style=\"text-align:right;\"> ({{ \"{:,.2f}\".format(i.balance or 0) }})</td>\n    </tr>\n    {% endfor %}\n  \n    <tr>\n        <td colspan=\"2\" style=\"text-align:left;\"></td>\n        <td colspan=\"5\" style=\"text-align:left;\"></td>\n        <td colspan=\"2\" style=\"text-align:center;\"></td>\n        <td colspan=\"2\" style=\"text-align:right;\"><b>{{ \"{:,.2f}\".format(balances.total.debit or 0) }}</b></td>\n        <td colspan=\"2\" style=\"text-align:right;\"><b>{{ \"{:,.2f}\".format(balances.total.credit or 0) }}</b></td>\n        <td colspan=\"2\" style=\"text-align:right;\"></td>\n    </tr>\n    <tr>\n        <td colspan=\"2\" style=\"text-align:left;\"></td>\n        <td colspan=\"5\" style=\"text-align:left;\"></td>\n        <td colspan=\"2\" style=\"text-align:center;\"></td>\n        <td colspan=\"2\" style=\"text-align:center;\"></td>\n        <td colspan=\"1\" style=\"text-align:center;\"><b>Balance:</b></td>\n        <td colspan=\"3\" style=\"text-align:right;\"> <b><u>{{ \"{:,.2f}\".format(balances.closing.balance or 0) }}</u></b></td>\n    </tr>\n    \n     <tr>\n        <td colspan=\"3\" style=\"text-align:left;\"><b>Generated By:</b></td>\n        <td colspan=\"5\" style=\"text-align:left;\"></td>\n        <td colspan=\"1\" style=\"text-align:center;\"></td>\n        <td colspan=\"2\" style=\"text-align:center;\"></td>\n        <td colspan=\"1\" style=\"text-align:center;\"></td>\n        <td colspan=\"3\" style=\"text-align:right;\"></td>\n    </tr>\n    \n\n</table>",
